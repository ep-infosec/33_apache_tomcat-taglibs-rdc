  <rule id="lowercaseletter">
    <item repeat="0-1"> <ruleref uri="#lowercase"/> </item>
    <one-of>
      <item> <ruleref uri="#a"/> <tag> $="a" </tag> </item>
      <item> <ruleref uri="#b"/> <tag> $="b" </tag> </item>
      <item> <ruleref uri="#c"/> <tag> $="c" </tag> </item>
      <item> <ruleref uri="#d"/> <tag> $="d" </tag> </item>
      <item> <ruleref uri="#e"/> <tag> $="e" </tag> </item>
      <item> <ruleref uri="#f"/> <tag> $="f" </tag> </item>
