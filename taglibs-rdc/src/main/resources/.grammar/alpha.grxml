      <item> <ruleref uri="#r"/> <tag> $="r" </tag> </item>
      <item> <ruleref uri="#s"/> <tag> $="s" </tag> </item>
      <item> <ruleref uri="#t"/> <tag> $="t" </tag> </item>
      <item> <ruleref uri="#u"/> <tag> $="u" </tag> </item>
      <item> <ruleref uri="#v"/> <tag> $="v" </tag> </item>
      <item> <ruleref uri="#w"/> <tag> $="w" </tag> </item>
      <item> <ruleref uri="#x"/> <tag> $="x" </tag> </item>
      <item> <ruleref uri="#y"/> <tag> $="y" </tag> </item>
      <item> <ruleref uri="#z"/> <tag> $="z" </tag> </item>
    </one-of>
  </rule>
      
  <rule id="uppercaseletter">
